{
  "name": "commoditenetur",
  "version": "1.0.0",
  "description": "Render Markdown as Solid JS components",
  "keywords": [
    "ad",
    "unified",
    "markdown",
    "commonmark",
    "amet",
    "ast",
    "odio",
    "solid-js",
    "et",
    "officia"
  ],
  "bugs": {
    "url": "https://github.com/KurtisSpencer/commoditenetur.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KurtisSpencer/commoditenetur.git"
  },
  "license": "MIT",
  "author": "SigurdGutmann",
  "type": "module",
  "main": "dist/index.jsx",
  "source": "lib/index.tsx",
  "browser": "dist/index.browser.jsx",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md,yaml}": [
      "prettier --write"
    ],
    "*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "@types/hast": "^2.3.4",
    "@types/prop-types": "^15.7.5",
    "@types/unist": "^2.0.6",
    "comma-separated-tokens": "^2.0.3",
    "property-information": "^6.2.0",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "solid-js": "^1.6.15",
    "space-separated-tokens": "^2.0.2",
    "style-to-object": "^0.4.1",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "vfile": "^5.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/hast": "^2.3.4",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-solid": "^0.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "solid-js": "^1.2.0"
  }
}